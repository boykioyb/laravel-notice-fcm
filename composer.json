{
    "name": "boykioyb/laravel-notice-fcm",
    "description": "Laravel send notification by fcm v1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoa Quang",
            "email": "hoatq.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/encryption": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "hashids/hashids": "^3.0|^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Boykioyb\\Notify\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boykioyb\\Notify\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Boykioyb\\Notify\\Providers\\AppServiceProvider",
                "Boykioyb\\Notify\\Providers\\EventServiceProvider"
            ]
        }
    }
}
